{
  "ignores": ["css",
  "js",
  "json",
  "img"],
  "mappings": [{
    "src": "scss",
    "dest": "css"
  },
  {
    "src": "src",
    "dest": "js"
  }],
  "options": {
    "autocompile": true,
    "outputStyle": "normal",
    "lineComments": false,
    "debugInfo": false,
    "strictMath": false,
    "strictUnits": false,
    "sourceMap": true
  },
  "customOptions": [],
  "includePaths": []
}
